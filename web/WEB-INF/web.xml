<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MemberServlet</servlet-name>
        <servlet-class>.MemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookServlet</servlet-name>
        <servlet-class>bookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookIssueServlet</servlet-name>
        <servlet-class>BookIssueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>memServlet</servlet-name>
        <servlet-class>memServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MemberServlet</servlet-name>
        <url-pattern>/MemberServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookServlet</servlet-name>
        <url-pattern>/bookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookIssueServlet</servlet-name>
        <url-pattern>/BookIssueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>memServlet</servlet-name>
        <url-pattern>/memServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>newhtml1.html </welcome-file>
    </welcome-file-list>
</web-app>
